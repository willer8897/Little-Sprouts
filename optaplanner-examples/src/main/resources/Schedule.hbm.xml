<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="org.lsdt.optaplannerLittleSprouts.Schedule" table="Schedule">
      <meta attribute="class-description">
         This class contains the schedule details. 
      </meta>
      <id name="id" type="int" column="account_id">
         <generator class="native"/>
      </id>
      <property name="date" column="date" type="date"/>
      <property name="room" column="room_id" type="int"/>
      <property name="time_start" column="time_start" type="string"/>
      <property name="time_end" column="time_end" type="string"/>
   </class>
</hibernate-mapping>